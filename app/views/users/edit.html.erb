<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <label for="inputProfile">introduction</label>
  <%= f.text_field :introduction, autofocus: true, id:"inputProfile", placeholder:"自己紹介"%>
  <%= f.submit "変更を保存" %>
<% end %>